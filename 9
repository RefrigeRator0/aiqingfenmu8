#define _CRT_SECURE_NO_WARNINGS 1//尽量开头引用，防止出错
#include <stdio.h>//引用数据库函数
int jiecheng(int x)
{
	if (x != 0)
	{
		return x * jiecheng(x - 1);
	}
	else
		return 1;
}
int main()
{
	int n = 0;
	int ret = 0;
	scanf("%d", &n);
	ret=jiecheng(n);
	printf("%d", ret);
	return 0;
}

